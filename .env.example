# Frontend Environment Variables (Vite)
VITE_API_URL=http://localhost:3001
VITE_DEMO_MODE=true

# Backend Environment Variables
NODE_ENV=development
PORT=3001
CORS_ORIGIN=http://localhost:5173,http://localhost:3000

# Raindrop Platform (Required for Hackathon)
RAINDROP_API_KEY=your_raindrop_api_key_here
RAINDROP_PROJECT_ID=your_raindrop_project_id_here

# Vultr Services (Required for Hackathon)
VULTR_API_KEY=your_vultr_api_key_here
VULTR_ML_ENDPOINT=https://your-vultr-ml-endpoint.com
VULTR_POSTGRES_URL=postgresql://user:password@host:port/database
VULTR_VALKEY_URL=redis://user:password@host:port

# ElevenLabs Voice AI (Required for Voice Agent Category)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=your_voice_id_here

# WorkOS Authentication (Optional - for launch-ready quality)
WORKOS_API_KEY=your_workos_api_key_here
WORKOS_CLIENT_ID=your_workos_client_id_here

# Stripe Payments (Optional - for launch-ready quality)
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret_here

# Supabase (Optional - for data storage)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
